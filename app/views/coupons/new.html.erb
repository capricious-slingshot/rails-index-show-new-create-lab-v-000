<h1>Coupon Form</h1>

<%= form_tag coupons_path do %>
  <p>
    <label for="coupon[store]">Coupon Store:</label>
    <input type="text" name="coupon[store]" id="coupon[coupon_store]">
  </p>
  <p>
    <label for="coupon[coupon_code]">Coupon Code:</label>
    <input type="text" name="coupon[coupon_code]" id="coupon[coupon_code]">
  </p>
  <%= submit_tag "Submit Coupon" %>
<% end %>
